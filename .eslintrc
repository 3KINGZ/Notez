{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react", "prettier"],
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "quotes": "off",
    "react/prop-types": ["off"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      },
      {
        "singleQuote": false
      }
    ],
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}
